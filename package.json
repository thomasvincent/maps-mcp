{
  "name": "maps-mcp",
  "version": "1.0.0",
  "description": "MCP server for Apple Maps on macOS - search locations, get directions, and open maps via the Model Context Protocol",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "maps-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "prepare": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "mcp",
    "apple-maps",
    "maps",
    "directions",
    "macos",
    "model-context-protocol",
    "ai",
    "claude"
  ],
  "author": "Thomas Vincent",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist"
  ]
}
